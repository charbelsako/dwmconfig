# Increase volume
"pactl set-sink-volume 0 +5%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume 0 -5% || kill -s 22"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute 0 toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute 0 toggle"
   XF86AudioMicMute

"xbacklight -inc 10" 
   XF86MonBrightnessUp

"xbacklight -dec 10"
   XF86MonBrightnessDown



